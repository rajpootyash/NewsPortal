            @Html.ActionLink("GCS News", "Index", "Home", new { area = "" }, new { @class = "navbar-brand Name", @style = "color: var(--Color--);font-size:1.5rem;font-weight: 700;" })
            <button style=" background: cornsilk;" type="button" class="navbar-toggler" data-bs-toggle="collapse"  data-bs-target=".navbar-collapse" title="Toggle navigation" aria-controls="navbarSupportedContent"
