            </button>
            <div class="collapse navbar-collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1" id="Navbar">

                    <li>@Html.ActionLink("होम", "Index", "Home", new { area = "" }, new { @class = "nav-link", @style = "color:var(--Color--)" })</li>
                    @foreach (var item in categories)
                    {
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" style="color:var(--Color--)" href="/Home/GetNewsAllCategoriWise?Categories=@item.Value" onclick="GetCatNews(@item.Value)" value="@item.Value" data-bs-toggle="dropdown" aria-expanded="false">
                               @item.CategriesName
                            </a>
                            @if (item.SubCategories != null)
                            {
                                if (item.SubCategories.Count > 0)
                                {


                                    <ul class="dropdown-menu">
                                        @foreach (var items in item.SubCategories)
                                        {
                                            <li><a class="dropdown-item"  href="/Home/GetNewsAllCategoriWise?sub=@item.Value"  >@items.Text</a></li>
                                        }


                                        @*<li><hr class="dropdown-divider"></li>*@

                                    </ul>
                                }
                            }

                        </li>

                    }

                </ul>
            </div>
        </div>
    </nav>
